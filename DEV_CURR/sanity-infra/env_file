#!/usr/bin/env bash
#Environment file
set -a

#Prevent *.pyc / __pycache Creation
# If this is set to a non-empty string, Python wonâ€™t try to write .pyc files on the import of source modules. This is equivalent to specifying the -B option.
PYTHONDONTWRITEBYTECODE=1

#Environment
GIT_REPO=/ws/cbhagwat-sjc/repos/sanity-infra
PYTHONPATH=${GIT_REPO}:$GIT_REPO/scheduler:$GIT_REPO/submitter:$GIT_REPO/utils
